<Window x:Class="HtmlParse.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HtmlParse"
        mc:Ignorable="d"
        Title="FindAll" Height="450" Width="800">
    <Grid Background="LightGray">


        <TabControl x:Name="tabControl" HorizontalAlignment="Stretch" Height="auto" Margin="0,0,0,0" VerticalAlignment="Stretch" Width="auto" Background="LightGray">
            <TabItem Header="Parser">
                <local:Parser/>
            </TabItem>
            <TabItem Header="Search">
                <local:Searcher/>
            </TabItem>
        </TabControl>

    </Grid>
</Window>